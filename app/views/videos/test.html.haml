#services
  .container
    .row
/ start gallery
#gallery.text-center
  .container
    .row.text-center
      .tabs.tabs-style-linebox.wow.fadeInUp{"data-wow-duration" => "2s"}
        %nav
          %ul.filters
            %li.hvr-sweep-to-top.active.filter{"data-filter" => "all"}
              %span All
            %li.hvr-sweep-to-top.filter{"data-filter" => ".girlsPub"}
              %span Girls Pub
            %li.hvr-sweep-to-top.filter{"data-filter" => ".danceParty"}
              %span Dance Party
            %li.hvr-sweep-to-top.filter{"data-filter" => ".videos"}
              %span Videos
  .container-fluid
    .row.gallery.text-center
      .col-md-12.noPadding
        .gallery-photo-grid
          .gallery-photos
            .gallery-photos-thumb

              - if @videos.any?
                - @videos.in_groups_of(3) do |group|
                  - group.take(5).collect do |video|
                    .gallery-photo-item.mix.danceParty.wow.fadeIn
                      %a.pretty-photo{"data-gal" => "prettyPhoto[gallery-photo-thumb]", :href => "https://img.youtube.com/vi/#{video.uid}/mqdefault.jpg"}
                        .photo-thumb
                          %img{:alt => "image-1", :src => "https://img.youtube.com/vi/#{video.uid}/mqdefault.jpg"}
                        .overlay
                          %span.gallery-icon
                            %i.fa.fa-search-plus.fa-2x
                          
